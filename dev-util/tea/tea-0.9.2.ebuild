DESCRIPTION="A command line tool to interact with Gitea servers"
HOMEPAGE="https://gitea.com/gitea/tea"
SRC_URI="
	https://gitea.com/gitea/${PN}/archive/v${PV}.tar.gz
		-> ${P}.tar.gz
	${EGO_SUM_SRC_URI}
"

LICENSE="MIT"
SLOT="0"
KEYWORDS="~amd64 ~x86"

S="${WORKDIR}/${PN}"

src_compile() {
	ego build -ldflags "-X main.Version=${PV} -w" -o tea
}

src_test() {
	ego test -tags='sqlite sqlite_unlock_notify' ./...
}

src_install() {
	dobin tea
}
